/* pages/components/centerLoading/index.wxss */
.center-loading{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading-box{
  width: 50px;
  height: 50px;
  background: #666;
  border-radius: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}